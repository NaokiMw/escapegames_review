<h3>ユーザー情報一覧</h3>

<table>
  <thead>
    <tr>
      <th>H.N</th>
      <th>title</th>
      <th>難易度</th>
      <th>ストーリー</th>
      <th>満足度</th>
      <th>ひらめき</th>
      <th>意外性</th>
      <th>place</th>
      <th>体験日</th>
      <th>開始時間</th>
      <th>結果</th>
      <th>photo</th>
      <th>更新日</th>
      <th>作成日</th>
    </tr>
  </thead>

  <tbody>
    <% @reviews.each do |review| %>
      <tr>
        <td><%= review.user.name %></td>
        <td><%= review.title %></td>
        <td><%= review.review_1 %></td>
        <td><%= review.review_2 %></td>
        <td><%= review.review_3 %></td>
        <td><%= review.review_4 %></td>
        <td><%= review.review_5 %></td>
        <td><%= review.place %></td>
        <td><%= review.play_day %></td>
        <td><%= review.start_time %></td>
        <td><%= review.result %></td>
        <td><%= review.image %></td>
        <td><%= review.updated_at %></td>
        <td><%= review.created_at %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<p><%= link_to "イベント新規登録", new_review_path %></p>
